{
  "name": "vitest-plugin-rsc",
  "version": "0.0.0-experimental-64e35ee-20250807",
  "description": "React Server Components (RSC) support for Vitest.",
  "keywords": [
    "vite",
    "react",
    "react-server-components",
    "rsc"
  ],
  "author": "Kasper Peulen",
  "license": "MIT",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    "./dist/nextjs/navigation": {
      "react-server": "./dist/nextjs/navigation.react-server.js",
      "default": "./dist/nextjs/navigation.js"
    },
    ".": "./dist/index.js",
    "./*": "./dist/*.js"
  },
  "imports": {
    "#navigation": {
      "react-server": "./test/navigation.react-server.ts",
      "default": "./test/navigation.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest",
    "dev": "tsdown --sourcemap --watch src",
    "build": "tsdown",
    "prepack": "tsdown"
  },
  "dependencies": {
    "@vitejs/plugin-rsc": "0.4.16",
    "@vitest/spy": "^3.2.4"
  },
  "devDependencies": {
    "@types/node": "^22.16.3",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/estree": "^1.0.8",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tsdown": "^0.12.9",
    "next": "^15.4.5"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*",
    "vite": "*",
    "next": "*"
  },
  "peerDependenciesMeta": {
    "next": {
      "optional": true
    }
  }
}
